<h1><%= title %></h1>

<form action="/register" method="POST" id="myform">
    <label for="id">Name</label>
    <input type="text" name="id" placeholder="Enter ID" id="id"> <span id="iderr"></span>
    <br>
    <label for="fname">First name </label>
    <input type="text" name="fname" placeholder="Enter First name" id="fname"> <span id="fnameerr"></span>
    <br>
    <label for="lname">Last name </label>
    <input type="text" name="lname" placeholder="Enter Last name" id="lname"> <span id="lnameerr"></span>
    <br>
    <label for="name">Email</label>
    <input type="email" name="email" placeholder="Enter Email" id="email"> <span id="emailerr"></span>
    <br>
    <label for="pass">Password</label>
    <input type="password" name="pass" placeholder="Enter Password" id="pass"> <span id="passerr"></span>
    <br>
    <label for="cpass">Confirm Password</label>
    <input type="password" name="cpass" placeholder="Enter Password" id="cpass"> <span id="cpasserr"></span>
    <br>
    <select name="dept" id="dept">
        <option value="bcs">BCS & IT</option>
        <option value="science&arts">science & arts </option>
        <option value="economic">economic</option>
    </select>
    <br>
    <input type="submit" name="submit" value="create" id="sub">
    <input type="submit" name="submit" value="update" id="del">
</form>

<table>
    <thead>
        <th>Id</th>
        <th>fname</th>
        <th>lname</th>
        <th>email</th>
        <th>dept</th>
    </thead>
    <tbody>
</table>


<script type="text/javascript" src="../js/validation.js"></script>

